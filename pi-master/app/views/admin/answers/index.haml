#datepicker.input-daterange.datatable
  = text_field_tag :from, nil, placeholder: 'start', id: 'from', value: @start_date
  %span.add-on to
  = text_field_tag :to, nil, placeholder: 'end', id: 'to', value: @end_date

%table.table.table-hover.table-responsive.list-table.answers-list
  %thead
    %tr
      %th= 'Date'
      %th= 'Time'
      %th= 'Question'
      %th= 'Response'
      %th= 'Question ID'
      %th= 'Response ID'
      %th= 'Next Question ID'
      %th= 'Survey ID'
      %th= 'Pageview Count'
      %th= 'Visit Count'
      %th= 'IP Address'
      %th= 'Device Type'
      %th= 'Device UDID'
      %th= 'Client Key'
      %th= 'Completion URL'
      %th= 'Custom Data'
      %th= 'User Agent'
  %tfoot
    %tr
      %th.empty
      %th.empty
      %th.empty
      %th.empty
      %th= 'Question ID'
      %th= 'Response ID'
      %th= 'Next Question ID'
      %th=' Survey ID'
