  \-
  %a.toggle-vis{'data-column': '1'} Time
  \-
  %a.toggle-vis{'data-column': '2'} Question
  \-
  %a.toggle-vis{'data-column': '3'} Response
  \-
  %a.toggle-vis{'data-column': '4'} Question ID
  \-
  %a.toggle-vis{'data-column': '5'} Response ID
  \-
  %a.toggle-vis{'data-column': '6'} Next Question ID
  \-
  %a.toggle-vis{'data-column': '7'} Survey ID
  \-
  %a.toggle-vis{'data-column': '8'} Pageview Count
  \-
  %a.toggle-vis{'data-column': '9'} Visit Count
  \-
  %a.toggle-vis{'data-column': '10'} IP Address
  \-
  %a.toggle-vis{'data-column': '11'} Device Type
