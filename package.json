{
  "scripts": {
    "dev": "bash scripts/launch/preview.sh",
    "start": "bash scripts/launch/preview.sh",
    "start:lite": "bash scripts/launch/preview-lite.sh",
    "build": "node scripts/build/preview-data.js && node scripts/build/demo-data.js",
    "build:widgets": "node scripts/build/generate-widgets.js",
    "test": "bash tests/run-all.sh",
    "test:unit": "node tests/unit/config/config.test.js && node tests/unit/lib/errors.test.js && node tests/unit/lib/logger.test.js && node tests/unit/lib/validators.test.js && node tests/unit/proxy/url-rewriting.test.js",
    "test:integration": "node tests/integration/preview/bridge.contract.test.mjs && node tests/integration/preview/surveyBridge.integration.test.mjs",
    "test:preview": "node tests/integration/preview/*.test.mjs",
    "test:demo-proxy": "node tests/integration/preview/demo-proxy.test.mjs",
    "test:url-rewriting": "node tests/integration/preview/url-rewriting.test.mjs",
    "test:script-interception-unit": "node tests/unit/proxy/script-interception.test.js",
    "test:script-injection": "node tests/integration/preview/script-injection.test.mjs",
    "test:script-interception": "node tests/integration/preview/script-interception.test.mjs",
    "clean": "bash scripts/cleanup.sh",
    "clean:all": "CLEAN_BUILD=1 CLEAN_NODE_MODULES=1 bash scripts/cleanup.sh"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "jsdom": "^27.0.1",
    "stripe": "^16.5.0",
    "wrangler": "^4.45.1"
  }
}
